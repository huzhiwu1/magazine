<scroll-view class="container" scroll-x="{{true}}" scroll-into-view="{{activeId}}">
    <text class="type {{index===magazineIndex?'active':''}}" 
     wx:for="{{magazineType}}"
     catch:tap="onTap"
     data-index="{{index}}"
     id="{{'magazine'+index}}">{{item}}</text>
</scroll-view>
