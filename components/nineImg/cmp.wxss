/* components/nineImg/cmp.wxss */
.container{
    width:710rpx;
}
.img-box image{
    width:230rpx;
    height:230rpx;
    margin-top:10rpx;
    margin-right:10rpx;
}
.img-box{
    margin-bottom:20rpx;
    height:710rpx;
    overflow: hidden;
    position:relative;
}
.img-box image:nth-of-type(3n){
    margin-right:0rpx;
}
.mainText{
    
    font-size:36rpx;
    font-weight:500;
    letter-spacing: 1rpx;
    line-height:50rpx;
}
.smallTitle{
    margin-top:20rpx;
    font-size:28rpx;
    font-weight:400;
    letter-spacing: 1rpx;
    line-height:40rpx;
    color:#666;
}
.img-num{
    display:block;
    background-color:rgba(0,0,0,.3);
    width:230rpx;
    text-align:center;
    line-height:230rpx;
    height:230rpx;
    color:#FFF;
    
    font-size:50rpx;
    position:absolute;
    bottom:-23rpx;
    right:0rpx;
    z-index:1;
    
}